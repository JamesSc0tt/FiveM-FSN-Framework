/*
	Main computer
*/
body {
	overflow: hidden
}
* {
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: -moz-none;
    -o-user-select: none;
    user-select: none;
}
.mdt_computer {
	width: 1122px;
	height: 904px;
	margin-left: auto;
	margin-right: auto;
	display: none;
}
.mdt_computer img {
	position:absolute;
	z-index: 1;
}
.computer_screen {
	position: absolute;
	float: left;
	z-index: 2;
	width: 1031px;
	height: 556px;
	margin-top: 42px;
	margin-left: 47px;
	background-image: url('images/background.png');
}

/*
	WINDOW
*/
.window {
	width: 100%;
	height: 100%;
	background-color: #d1d1d1;
}
.window_titlebar {
	height: 22px;
	line-height: 22px;
	padding: 2.5px;
	width: 1027px;
	background-color: black;
	color: white;
	font-family: Arial;
}
.window_titlebar img {
	height: 18px;
	width: 18px;
	margin: 2px;	
}
.window_titlebar_title {
	height: 100%;
	width: 50%;
	float: left;
	margin-left: 25px;
	line-height: 22px;
	font-size: 12px;
}
.window_titlebar_button {
	height: 21px;
	line-height: 21px;
	width: 35px;
	float: right;
	text-align: center;
	background-color: red;
	color: white;
	font-family: Arial;
	font-weight: bold;
}
.window_application {
	height: 529px;
	width: 100%;
	float: left;
	overflow-y: scroll;
}

/*
	Start menu
*/
.startmenu {
	position: absolute;
	z-index: 3;
	width: 250px;
	height: 400px;
	background-color: white;
	margin-top: 198px;
	margin-left: 47px;
	display: none;
}
.startmenu_sidebar {
	height: 100%;
	background-color: black;
	width: 40px;
	float: left;
}
.startmenu_sidebar_offbutton {
	margin-top: 350px;
	width: 40px;
}
.startmenu_sidebar_offbutton img {
	width: 30px;
	margin: 5px;
}
.startmenu_programs {
	width: 210px;
	float: left;
}
.startmenu_program {
	width: 100%;
	height: 35px;
	border-bottom: 1px solid #e8e8e8;
}
.startmenu_program_icon {
	width: 30px;
	height: 30px;
	margin-right: 10px;
	float: left;
}
.startmenu_program_icon img {
	width: 25px;
	height: 25px;
	margin-top: 1px;
	margin-left: 5px;
}
.startmenu_program_title {
	font-family: Arial;
	font-weight: bold;
	margin-top: 8px;
	margin-left: 10px;
}
.startmenu_program_desc {
	font-family: Arial;
	margin-left: 10px;
	margin-bottom: 10px;
	font-size: 10px;
	color: #c6c6c6;
}

/*
	Task bar
*/
.computer_taskbar {
	width: 1031px;
	z-index: 3;
	float: left;
	height: 30px;
	margin-top: 598px;
	margin-left: 47px;
	background-color: black;
}
.taskbar_windowsbutton {
	width: 30px;
	height: 30px;
	float: left;
}
.taskbar_windowsbutton img {
	width: 20px;
	height: 20px;
	margin: 5px;
}

.taskbar_timeandshit {
	height: 30px;
	width: 50px;
	float: right;
}
.taskbar_timearea {
	font-family:Arial;
	color: white;
	height: 100%;
	font-size: 12px;
	padding: 8px;
	float: right;
}
.taskbar_desktopbutton {
	width: 3px;
	height: 100%;
	float: right;
	border-left: 1px solid #1c1c1c;
}

/*
	DMV Screen
*/
#dmv-window {
	display: none
}

/*
	Warrants Screen
*/
#warrant-window {
	display: none
}

/*
	Dispatch Screen
*/
#dispatch-window {
	display: none
}
.dispatch_searcharea {
	margin-top: 20px;
	margin-left: 40px;
	margin-right: 40px;
	width: 947px;
	height: 30px;
	background-color:red;
}
.dispatch_searchbar {
	width: 100%;
	height: 100%;
	padding:2px;
	font-family: Courier New;
	float: left;
}
.dispatch_results {
	margin-top: 10px;
	margin-left: 40px;
	margin-right: 40px;
	width: 947px;
}


/*
	CPIC Screen
*/
#cpic-window {
	display: none
}
.cpic_title {
	margin-top: 40px;
	margin-left: 40px;
	font-family: Arial;
	font-weight: bold;
	font-size: 26px;
}
.cpic_desc {
	font-family: Courier New;
	font-size: 13px;
	margin-left: 40px;
	margin-right: 40px;
	margin-top: 5px;
}
.cpic_searcharea {
	margin-top: 20px;
	margin-left: 40px;
	margin-right: 40px;
	width: 947px;
	height: 60px;
	background-color:red;
}
.cpic_searchbar {
	width: 100%;
	height: 50%;
	padding:2px;
	font-family: Courier New;
	float: left;
}
.cpic_searchbutton {
	width: 100%;
	height: 50%;
	float: left;
}
.cpic_results {
	margin-top: 10px;
	margin-left: 40px;
	margin-right: 40px;
	width: 947px;
	display: none;
}

/*
	Booking Screen
*/
#booking-window {
	display: none;
}
.booking_inputarea {
	margin-top: 10px;
	margin-left: 40px;
	margin-right: 40px;
	width: 947px;
	font-family: Arial;
}
.booking_inputarea_reset {
	width: 100%;
	height: 40px;
	background-color: red;
	font-weight: bold;
	color: white;
	font-family: Arial;
	font-size: 25px;
	border-color: red;
	margin-bottom: 10px;
}
.booking_charges {
	width: 100%;
	height: auto;
	margin-top: 10px;
	float: left;
	font-family:Courier New;
	font-weight:bold;
}
.booking_charges button {
	width: 100%;
}
.booking_charges textarea {
	width: 100%;
	height: 70px;
	resize: none;
}
.booking_playerid {
	width: 30%;
	float: left;
	height: 90px;
	font-family:Courier New;
	font-weight:bold;
	margin-right: 47px;
}
.booking_playerid input {
	width: 100%;
	height: 30px;
}
.booking_playername {
	width: 30%;
	float: left;
	height: 90px;
	font-family:Courier New;
	font-weight:bold;
}
.booking_playername input {
	width: 100%;
	height: 30px;
}
.booking_playername span {
	font-size: 11px;
	color:red;
}
.booking_officername {
	width: 30%;
	float: left;
	height: 90px;
	font-family:Courier New;
	font-weight:bold;
	margin-left: 47px;
}
.booking_officername input {
	width: 100%;
	height: 30px;
}
.booking_officername span {
	font-size: 11px;
	color:red;
}
.booking_description {
	width: 100%;
	font-family:Courier New;
	font-weight:bold;
}
.booking_description textarea {
	width: 100%;
	height: 60px;
	resize: none;
}
.booking_description span {
	font-size: 11px;
	color:red;
}
.booking_buttons {
	margin-top: 10px;
	float: left;
	width: 100%;
	height: 40px;
}
.booking_buttons_warrant {
	background-color: orange;
	color: white;
	font-weight: bold;
}
.booking_buttons_apply {
	background-color: green;
	color: white;
	font-weight: bold;
	float: right;
}
.booking_chargesarea {
	width: 100%;
	margin-top: 10px;
	float:left
	height: 100px;
}
.booking_dropdown {
	margin: 10px;
	margin-left: 25px;
	float: left;
	width: 45%;
	min-height: 30px;
	border-radius: 3px;
	background-color: #e0e0e0;
}
.booking_dropdown_title {
	width: 74%;
	padding-left:10px;
	height: 30px;
	float: left;
	line-height: 30px;
}
.booking_dropdown_icon {
	width: 10%;
	line-height: 30px;
	float:right;
	height: 30px;
	display: table-cell;
    vertical-align: middle;
}
.booking_dropdown_icon i {
	margin-top: 4px;
}
.booking_dropdown_hidden {
	width: calc(100% - 1px);
	float: left;
	height: auto;
	border: 1px solid #e0e0e0;
	background-color:white;
	display: none;
}
.booking_dropdown_charge_title {
	width: 84%;
	height: 21px;
	padding: 2px;
	float: left;
}
.booking_dropdown_charge_add {
	width: 15%;
	height: 25px;
	float: left;
}
.booking_dropdown_charge_plus {
	background-color: green;
	margin-right: 3px;
	width: 45%;
	height: 90%;
	color: white;
	border-color: green;
}
.booking_dropdown_charge_minus {
	background-color: red;
	color: white;
	width: 45%;
	height: 90%;
	border-color: red;
}

/*
	table
*/
table {
    border-collapse: collapse;
	width: 100%;
	font-family: Courier New;
}
table, th, td {
    border: 1px solid black;
}
td {
	font-size: 11px;
}